{
  "name": "react-cm",
  "description": "manages your project components from cli",
  "version": "1.0.0",
  "author": "kostayne",
  "bin": {
    "cli-pm": "./bin/run"
  },
  "bugs": "https://github.com/Kostayne/react-cm/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/cli": "^0.11.19",
    "@types/rimraf": "^3.0.0",
    "oclif": "^1.16.1",
    "pascal-case": "^3.1.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.54",
    "@types/react": "^17.0.2",
    "chai": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Kostayne/react-cm",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cli-pm",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Kostayne/react-cm",
  "scripts": {
    "dev": "tsc",
    "generate-template": "oclif multi",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
